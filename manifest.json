{
    "name": "Yegg",
    "description": "Show key image as grid view",
    "version": "0.0.1",

    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*", "file:///*"],
        "run_at": "document_end",
        "js": ["content.js"]
    }],

    "icons": {
       "128": "icon128.png"
    },

    "permissions": ["activeTab"],
    "manifest_version": 3
}